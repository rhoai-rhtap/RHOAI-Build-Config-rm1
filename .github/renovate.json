{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "customManagers": [
        {
            "fileMatch": [
                "bundle/csv-patch.yaml"
            ],
            "customType": "regex",
            "datasourceTemplate": "docker",
            "matchStrings": [
                "(?<depName>ose-oauth-proxy-image): (?<imagePath>registry\\.redhat\\.io/openshift4/ose-oauth-proxy)@(?<currentValue>sha256:[a-f0-9]+)"
            ],
            "versioningTemplate": "docker"
        }
    ]
}
