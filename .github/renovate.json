{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^bundle/csv-patch\\.yaml$"],
      "matchStrings": [
  "(?<depName>registry.redhat.io/openshift4/ose-oauth-proxy)@sha256:(?<currentDigest>[a-f0-9]+) # (?<currentValue>[v0-9.]+)"
],
      "autoReplaceStringTemplate": "{{{depName}}}: registry.redhat.io/openshift4/ose-oauth-proxy@{{{newValue}}} # {{{newVersion}}}",
      "datasourceTemplate": "docker"
    }
  ],
  "schedule": ["at any time"]
}
