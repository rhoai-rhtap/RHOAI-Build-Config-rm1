{
    "packageRules": [
      {
        "matchPackageNames": ["registry.redhat.io/openshift4/ose-oauth-proxy"],
        "versioning": "loose"
      }
    ],
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": ["^bundle/csv-patch\\.yaml$"],
        "matchStrings": [
          "(?<depName>ose-oauth-proxy-image): (?<imagePath>registry\\.redhat\\.io/openshift4/ose-oauth-proxy)@sha256:(?<currentValue>[a-f0-9]+)"
        ],
        "datasourceTemplate": "docker",
        "currentValueTemplate": "sha256:{{currentValue}}"
      }
    ]
  }
