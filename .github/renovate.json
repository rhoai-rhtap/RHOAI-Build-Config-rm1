{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^bundle/csv-patch\\.yaml$"],
      "matchStrings": [
        "(?<depName>ose-oauth-proxy-image): registry\\.redhat\\.io/openshift4/ose-oauth-proxy@(?<currentDigest>sha256:[a-f0-9]+) # (?<currentValue>[v0-9.]+)"
      ],
      "autoReplaceStringTemplate": "{{{depName}}}: registry.redhat.io/openshift4/ose-oauth-proxy@{{{newValue}}} # {{{newVersion}}}",
      "datasourceTemplate": "docker"
    }
  ],
  "schedule": ["at any time"]
}
