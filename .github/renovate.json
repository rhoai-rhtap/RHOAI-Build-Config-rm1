{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "customManagers":
    [
        {
            "fileMatch": ["^bundle/csv-patch.yaml$"],
            "customType": "regex",
            "datasourceTemplate": "docker",
            "matchStrings":
            [
               "<depName>:<currentValue>@<currentDigest>"
            ],
            "versioningTemplate": "redhat"
        }
    ],
    "schedule": ["at any time"]
}
