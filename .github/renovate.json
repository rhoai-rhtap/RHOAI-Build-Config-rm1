{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "customManagers":
    [
        {
            "fileMatch": ["bundle/csv-patch.yaml"],
            "customType": "regex",
            "datasourceTemplate": "docker",
            "matchStrings":
            [
                "ose-oauth-proxy-image:\\s*(?<depName>registry\\.redhat\\.io/openshift4/ose-oauth-proxy)(?:@sha256:(?<currentDigest>[a-fA-F0-9]+))?"
            ],
            "versioningTemplate": "docker"
        }
    ]
}
